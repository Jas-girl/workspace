package com.libraryManagementSystem;

public class MyMainClass {
	public static void main(String[] args) {
		Book b1=new Book();
		b1.setTitle("Maths");
		b1.setAuthor("Cbse");
		b1.setISBN(234);
		b1.setQuantity(2);
		
		Book b2=new Book();
		b2.setTitle("English");
		b2.setAuthor("Cbse");
		b2.setISBN(2345);
		b2.setQuantity(2);
		
		Book b3=new Book();
		b3.setTitle("Punjabi");
		b3.setAuthor("Cbse");
		b3.setISBN(2341);
		b3.setQuantity(3);
		
		Book b4=new Book();
		b4.setTitle("Science");
		b4.setAuthor("Cbse");
		b4.setISBN(23);
		b4.setQuantity(2);
		
		Book b5=new Book();
		b5.setTitle("Computer");
		b5.setAuthor("Cbse");
		b5.setISBN(24);
		b5.setQuantity(2);
		
		
		Library li=new Library();
		li.addBooks(b1);
		System.out.println(b1);
		li.addBooks(b2);
		System.out.println(b2);
		li.addBooks(b3);
		System.out.println(b3);
		li.addBooks(b4);
		System.out.println(b4);
		li.addBooks(b5);
		System.out.println(b5);
		
	}
}
